<AnchorPane id="AnchorPane" fx:id="menuMainPane" prefHeight="300.0" prefWidth="300.0" style="-fx-background-color: ffe900ff;" styleClass="border-main" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="notes.menuController">
